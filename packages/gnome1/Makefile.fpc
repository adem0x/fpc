#
#   Makefile.fpc for gnome bindings
#

[package]
name=gnome1
version=1.0.0

[require]
libc=y
packages=gtk1 imlib

[target]
units=libgnomeui libzvt libart libgnome gconfclient gconf

[compiler]
includedir=src
sourcedir=src tests examples src src/zvt src/libgnome src/gconfclient src/gconf src/libgnomeui

[install]
fpcpackage=y

[default]
fpcdir=../..

[rules]
.NOTPARALLEL:
