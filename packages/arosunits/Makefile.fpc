#
#   Makefile.fpc for Free Pascal Amiga units Bindings
#

[package]
name=arosunits
version=2.7.1

[target]
units= gadtools workbench asl clipboard iffparse \
       mui amigalib timerutils diskfont icon

[compiler]
includedir=src
sourcedir=src

[install]
fpcpackage=y

[default]
fpcdir=../..

[rules]
.NOTPARALLEL:
