#
#   Makefile.fpc for libcurl Library interface
#

[package]
name=libcurl
version=2.2.1rc1

[require]
libc=y

[target]
units=libcurl
examples=testcurl teststream

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests


[shared]
build=n

[rules]
.NOTPARALLEL:
