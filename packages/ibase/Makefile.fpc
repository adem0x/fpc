#
#   Makefile.fpc for InterBase bindings
#

[package]
name=ibase
version=2.0.0

[target]
units=ibase40 ibase60 ibase60dyn
examples=testib40 testib60

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests

[shared]
build=n

[rules]
.NOTPARALLEL:
