#
#   Makefile.fpc for fpWeb tests
#

[require]
packages=fcl-base fcl-xml fcl-net

[target]
programs=rpcserv rpccli 
programs_linux=cdnslookup
programs_freebsd=cdnslookup
units=svrclass svrclass_xmlrpc

[compiler]
options=-S2h

[install]
fpcpackage=y

[default]
fpcdir=../../..


[rules]
.NOTPARALLEL:
