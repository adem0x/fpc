#
#   Makefile.fpc for MD5 checksum.
#

[package]
name=pxlib
version=2.0.0

[target]
units_linux=pxlib
units_win32=pxlib
units_darwin=pxlib
units_freebsd=pxlib
units_openbsd=pxlib
units_netbsd=pxlib
examples_linux=ppxview
examples_win32=ppxview
examples_darwin=ppxview
examples_freebsd=ppxview
examples_openbsd=ppxview
examples_netbsd=ppxview

[install]
fpcpackage=y

[compiler]
sourcedir=src

[default]
fpcdir=../..

[rules]
.NOTPARALLEL:
