#
#   Makefile.fpc for FCL Web components
#

[package]
name=fcl-web
version=2.2.2

[target]
units=httpdefs fphttp custweb custcgi fpcgi fptemplate fphtml websession fpweb \
      webutil fpdatasetform cgiapp ezcgi fpapache fastcgi custfcgi fpfcgi

rsts=fpcgi fphtml fpweb websession cgiapp

[require]
packages=fcl-base fcl-xml fcl-db fcl-process httpd22
packages_darwin=univint

[compiler]
options=-S2h
sourcedir=src

[install]
fpcpackage=y

[default]
fpcdir=../..


[rules]
.NOTPARALLEL:
