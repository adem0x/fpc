#
#   Makefile.fpc for FCL Web components
#

[package]
name=fcl-web
version=2.0.0

[target]
units=httpdefs fphttp custcgi fpcgi fptemplate fphtml websession fpweb \
      webutil fpdatasetform
rsts=fpcgi fphtml fpweb websession

[require]
packages=fcl-base fcl-xml fcl-db

[compiler]
options=-S2h
sourcedir=src

[install]
fpcpackage=y

[default]
fpcdir=../..


[rules]
.NOTPARALLEL:
