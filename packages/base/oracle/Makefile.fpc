#
#  Makefile.fpc for oracle units
#

[package]
name=oracle
version=2.0.0

[target]
units=oci ocidyn oratypes oraoci
exampledirs=example

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../../..

# in libdir variable must be path to libraries
# libclntsh.*,libnlsrtl3.*
#[compiler]
#libdir=/usr/local/oracle8cli/lib

[shared]
build=n
