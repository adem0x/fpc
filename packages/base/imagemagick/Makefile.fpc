#
#   Makefile.fpc for Free Pascal GTK 2.x.y Bindings
#

[package]
name=gtk2
version=2.0.0

[require]
libc=y

[target]
units=buildim 
exampledirs=
implicitunits=magick_wand imagemagick

[compiler]
unitdir=magick wand

[install]
buildunit=buildgtk2
fpcpackage=y

[shared]
build=n

[default]
fpcdir=../../..

[rules]
.NOTPARALLEL:
