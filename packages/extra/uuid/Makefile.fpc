#
#   Makefile.fpc for gdbm bindings
#

[package]
name=uuid
version=2.0.0

[target]
units=macuuid libuuid
examples=testuid testlibuid

[require]
package=rtl

[install]
examplesubdir=uuid
fpcpackage=y

[default]
fpcdir=../../..

[rules]
testuid$(EXEEXT): testuid.pp uuid$(PPUEXT)

testlibuid$(EXEEXT): testlibuid.pp libuuid$(PPUEXT)
