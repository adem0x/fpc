#
#   Makefile.fpc for Free Pascal libzvt 1.x Bindings
#

[package]
name=zvt
version=2.0.0

[require]
libc=y
packages=gtk imlib

[target]
units=libzvt
exampledirs=examples

[compiler]
sourcedir=zvt

[libs]
libname=libfpzvt.so
libversion=2.0.0

[install]
fpcpackage=y

[default]
fpcdir=../../../..

[shared]
build=n

[rules]
.NOTPARALLEL:
