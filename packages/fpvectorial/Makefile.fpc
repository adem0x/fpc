#
#   Makefile.fpc for FPVectorial Library
#

[package]
name=fpvectorial
version=2.2.2

[require]
libc=n

[target]
units=fpvectorial
exampledirs=
implicitunits=

[compiler]
includedir=src
sourcedir=src

[install]
buildunit=
fpcpackage=y

[shared]
build=n

[default]
fpcdir=../..

[rules]
.NOTPARALLEL:
