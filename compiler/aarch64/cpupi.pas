unit cpupi;

{$i fpcdefs.inc}

interface

  uses
    psub;

  type
    taarch64procinfo=class(tcgprocinfo)
      { no need to override anything, as the ABI requires us to use a frame
        pointer at all times }
    end;

implementation

  uses
    procinfo;

begin
  cprocinfo:=taarch64procinfo;
end.
