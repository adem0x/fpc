#
#   Makefile.fpc for FPDoc
#

[package]
name=instantfpc
version=3.1.1

[install]
fpcpackage=y

[require]
packages=fcl-process

[target]
programs=instantfpc

[compiler]
options=-S2h

[default]
fpcdir=../..

[clean]
units=instantfptools

[rules]
.NOTPARALLEL:
fpdoc$(EXEEXT): instantfpc.pas instantfptools.pas

