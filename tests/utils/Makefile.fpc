#
# Makefile.fpc for Free Pascal testing utilities
#

[install]
fpcpackage=y

[default]
fpcdir=../..

[require]
# We depend on the installed compiler with it's fpc.cfg
nortl=y

[target]
programs=dotest fail testfail digest prepup $(DBDIGEST)

[clean]
programs=dbdigest

[rules]

utils: all

utilsdb:
        $(MAKE) utils DBDIGEST=dbdigest
