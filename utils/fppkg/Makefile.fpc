#
#   Makefile.fpc for fppkg
#

[target]
programs=fppkg
units=fpmktype fpmkunit fprepos fpxmlrep pkgropts
examples=rep2xml
rsts=fprepos fpxmlrep fppkg

[install]
fpcpackage=y

[default]
fpcdir=../..

[require]
packages=fcl

[rules]
fpmkunit$(PPUEXT) : fpmkunit.pp fpmkcnst.inc fpmktype$(PPUEXT)

fpmktype$(PPUEXT) : fpmktype.pp fpmkcnst.inc
